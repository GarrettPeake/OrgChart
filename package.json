		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"react-devtools-core": "^4.19.1",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.0.3",
