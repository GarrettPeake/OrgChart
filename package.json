    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "mammoth": "^1.9.1",
    "meow": "^11.0.0",
    "openai": "^5.10.1",
    "pdf-parse": "^1.1.1",
    "pino": "^9.7.0",
    "react": "19.1.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/node": "^24.0.15",
    "@types/pdf-parse": "^1.1.5",
