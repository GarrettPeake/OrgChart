		"@parcel/watcher": "^2.5.1",
		"fullscreen-ink": "^0.1.0",
		"glob": "^11.0.3",
		"ignore": "^7.0.5",
		"ink": "5.2.0",
		"ink-big-text": "^2.0.0",
		"ink-text-input": "^6.0.0",
		"js-tiktoken": "^1.0.21",
		"mammoth": "^1.9.1",
		"marked-terminal": "^7.3.0",
		"meow": "^11.0.0",
		"pdf-parse": "^1.1.1",
		"pino": "^9.7.0",
		"react": "^18.3.1",
		"vite-node": "^3.2.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
