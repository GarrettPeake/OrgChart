{
	"name": "orgchart",
	"version": "1.0.0",
	"main": "src/App.tsx",
	"type": "module",
	"bin": {
		"orgchart": "./dist/src/App.js"
	},
	"scripts": {
		"build": "rm -rf dist && npm run format && tsc && tsc-alias && chmod +x ./dist/src/App.js",
		"dev": "vite-node --env-file=.env src/App.tsx",
		"dev:watch": "vite-node -w --env-file=.env src/App.tsx",
		"test": "vitest run",
		"format": "prettier -w ."
	},
	"author": "Garrett Peake",
	"license": "Apache-2.0",
	"description": "Assemble an organization of agents to complete a task",
